        "build": "nest build",
        "start": "nest start",
        "dev": "yarn start:dev",
        "start:dev": "nest start --watch -c ./nest-cli.json",
        "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run  -f database/config.ts",
        "seed:run": "ts-node ./node_modules/typeorm/cli.js migration:run  -f database/config.ts -c seed ",
        "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "start:prod": "node dist/src/main.js",
        "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop -f database/config.ts",
        "drop:migration": "yarn run schema:drop && yarn run migration:run",
        "wdio": "wdio run wdio.conf.js",
        "download-file": "sh download.sh",
        "postinstall": "husky install"
    },
